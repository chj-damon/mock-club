    "cookie-parser": "^1.4.3",
    "express": "^4.14.1",
    "express-session": "^1.15.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "log4js": "^1.1.0",
    "multer": "^1.3.0",
    "mysql": "^2.13.0",
