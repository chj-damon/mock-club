{
  "name": "mock-store",
  "version": "1.0.0",
  "description": "simulate tmall using react-related skills",
  "main": "index.js",
  "scripts": {
    "devdll": "rimraf lib/debug && cross-env NODE_ENV=development webpack --config webpack.dll.config.js --progress",
    "prodll": "rimraf lib/dist && cross-env NODE_ENV=production webpack --config webpack.dll.config.js --progress",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack",
    "dev": "cross-env NODE_ENV=development nodemon dev-server.js --exec babel-node --presets es2015,stage-0",
    "start": "cross-env NODE_ENV=production nodemon app.js --exec babel-node --presets es2015,stage-0"
  },
  "keywords": [
    "tmall",
    "react",
    "webpack",
    "mysql"
